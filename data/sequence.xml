<?xml version="1.0" encoding="utf-8"?>
<odoo>
 <data noupdate="1">
 
   <record id="clinic_register_sequence" model="ir.sequence">
      <field name="name">Register</field>
      <field name="code">clinic.register.sequence</field>
      <field name="active">TRUE</field>
      <field name="prefix">DFT/%(y)s/</field>
      <field name="padding">4</field>
      <!-- <field name="number_next">1</field> -->
      <field name="number_increment">1</field>
   </record>
 
   <record id="clinic_checkup_sequence" model="ir.sequence">
      <field name="name">Register</field>
      <field name="code">clinic.checkup.sequence</field>
      <field name="active">TRUE</field>
      <field name="prefix">PMR/%(y)s/</field>
      <field name="padding">4</field>
      <!-- <field name="number_next">1</field> -->
      <field name="number_increment">1</field>
   </record> 
   <record id="clinic_doctor_sequence" model="ir.sequence">
      <field name="name">Doctor</field>
      <field name="code">clinic.doctor.sequence</field>
      <field name="active">TRUE</field>
      <field name="prefix">DKT</field>
      <field name="padding">4</field>
      <!-- <field name="number_next">1</field> -->
      <field name="number_increment">1</field>
   </record> 
   <record id="clinic_patient_sequence" model="ir.sequence">
      <field name="name">Patient</field>
      <field name="code">clinic.patient.sequence</field>
      <field name="active">TRUE</field>
      <field name="prefix">PSN</field>
      <field name="padding">4</field>
      <!-- <field name="number_next">1</field> -->
      <field name="number_increment">1</field>
   </record> 
   <record id="clinic_invoice_sequence" model="ir.sequence">
      <field name="name">Invoice</field>
      <field name="code">clinic.invoice.sequence</field>
      <field name="active">TRUE</field>
      <field name="prefix">INV/%(y)s/%(month)s/</field>
      <field name="padding">4</field>
      <!-- <field name="number_next">1</field> -->
      <field name="number_increment">1</field>
   </record> 
   <record id="clinic_poly_sequence" model="ir.sequence">
      <field name="name">Poly</field>
      <field name="code">clinic.poly.sequence</field>
      <field name="active">TRUE</field>
      <field name="prefix">POLY</field>
      <field name="padding">3</field>
      <!-- <field name="number_next">1</field> -->
      <field name="number_increment">1</field>
   </record>
  </data>
 
</odoo>